<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="480.0" prefWidth="588.0" xmlns="http://javafx.com/javafx/8"
      fx:controller="org.tetram.bdtheque.gui.MainController">
    <children>
        <MenuBar id="menuBar" fx:id="menuBar">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#menuCloseClick" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <ToolBar id="toolBar" fx:id="toolBar" prefHeight="40.0" prefWidth="200.0">
            <items>
                <Button mnemonicParsing="false" text="Button"/>
            </items>
        </ToolBar>
    </children>
</VBox>
