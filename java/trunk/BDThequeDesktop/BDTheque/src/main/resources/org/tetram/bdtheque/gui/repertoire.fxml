                                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                AnchorPane.topAnchor="0.0"/>
                </children>
            </AnchorPane>
        </content>
    </Tab>
    <Tab id="tabAuteurs" fx:id="tabAuteurs" text="%Auteurs">
        <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <children>
                    <fx:include fx:id="auteurs" source="components/treeview.fxml" AnchorPane.bottomAnchor="0.0"
                                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                AnchorPane.topAnchor="0.0"/>
                </children>
            </AnchorPane>
        </content>
    </Tab>
    <Tab id="tabParabd" fx:id="tabParabd" text="%ParaBD">
